tmux has-session -t blog
if [ $? != 0 ]
then
     tmux new-session -s blog -n editor -d
     tmux new-window  -n server -t blog
     tmux new-window -n console -t blog
     tmux send-keys -t blog:1 'cd ~/blog' C-m
     tmux send-keys -t blog:1 'emacs resources/templates/md/posts/2015-02-20-childhood-theme-songs.md' C-m
     tmux send-keys -t blog:2 'cd ~/blog' C-m
     tmux send-keys -t blog:2 'lein ring server' C-m
     tmux send-keys -t blog:3 'cd ~/blog' C-m
fi

tmux attach -t blog