tmux has-session -t inferno
if [ $? != 0 ]
then
     tmux new-session -s inferno -n editor -d
     tmux new-window  -n client -t inferno
     tmux new-window -n test -t inferno
     tmux new-window -n console -t inferno
     tmux send-keys -t inferno:1 'cd ~/inferno' C-m
     tmux send-keys -t inferno:1 'emacs src/cljs/xanadu/pages/application.cljs ../crawlingchaos/src/clj/crawlingchaos/domain/installer/route.clj' C-m
     tmux send-keys -t inferno:2 'cd ~/inferno' C-m
     tmux send-keys -t inferno:2 'boot development' C-m
     tmux send-keys -t inferno:3 'cd ~/inferno' C-m
     tmux send-keys -t inferno:3 'boot autotest' C-m
     tmux send-keys -t inferno:4 'cd ~/inferno' C-m
fi

tmux attach -t inferno